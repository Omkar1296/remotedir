System.out.println("first login the website");
System.out.println("Enter book name Search bar");
String bname=sc.nextLine();
System.out.println("Click on search Button");
System.out.println("Click on buy button");
System.out.println("Enter quntity of books");
int bquntity =sc.nextInt();

if (bquntity<=3)
{
System.out.println("Enter the price of book");
int bprice=sc.nextInt();
if (bprice<=1000)
{
int totalbkprice=bprice*bquntity;
System.out.println("Price of book is "+totalbkprice);
}
else
{
System.out.println("price is out of limit");
}
System.out.println("It show the total price of book");
}
else
{
System.out.println("It shows blank page");
}
